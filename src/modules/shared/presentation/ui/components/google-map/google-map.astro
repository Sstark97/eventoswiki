---
export interface Props {
  src: string
  ariaLabel: string
}

const { src, ariaLabel } = Astro.props as Props
---

<iframe
  style="width: 100%; height: 400px;"
  height="400"
  allowfullscreen=""
  loading="lazy"
  referrerpolicy="no-referrer-when-downgrade"
  src={src}
  aria-label={ariaLabel}></iframe>
