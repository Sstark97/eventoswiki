{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/shared/*": ["./src/modules/shared/*"],
      "@/events/*": ["./src/modules/events/*"],
      "@/ui/*": ["./src/modules/shared/presentation/ui/*"],
      "@/*": ["./src/*"]
    }
  }
}
